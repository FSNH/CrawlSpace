{
  "spider": "currency",
  "type": "chemenu网站",
  "home": "https://www.chemenu.com/",
  "settings": {
    "user-agent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/101.0.4951.64 Safari/537.36",
    "SCHEDULER": "scrapy_redis.scheduler.Scheduler",
    "DUPEFILTER_CLASS": "scrapy_redis.dupefilter.RFPDupeFilter",
    "SCHEDULER_PERSIST": true,
    "REDIS_START_URLS_AS_ZSET": true,
    "REDIS_START_URLS_KEY": "chems:start_urls",
    "STATS_CLASS": "scrapy_redis.stats.RedisStatsCollector",
    "SCHEDULER_DUPEFILTER_KEY": "currency:chem:dupefilter",
    "SCHEDULER_QUEUE_KEY": "currency:chem:requests"
  },
  "start_urls": ["https://www.chemenu.com/"],
  "allowed_domains": ["chemenu.com"],
  "srules": "chemenu_rules",

  "rules": [
    {
      "link_extractor": {
        "restrict_css":".link_item.left0 dd"
      },
      "follow": true,
      "callback": "parse_item"
    }
  ],
  "item": {
    "class": "ChemItem",
    "loader": "ItemLoader",
    "nextpage": true,
    "next_rule": "#pagination_next a::attr('href')",
    "attrs": {
      "source": [
        {
          "method": "value",
          "arg": "chem"
        }
      ],
      "urls": [
        {
          "method": "css",
          "arg": ".cas_img a::attr(\"href\")"
        }
      ]

    }
  }
}