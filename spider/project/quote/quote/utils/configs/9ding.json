{
  "spider": "currency",
  "type": "九鼎网站",
  "home": "http://www.9dingchem.com/",
  "settings": {
    "user-agent": "Mozilla/5.0 (X11; Linux x86_64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/101.0.4951.64 Safari/537.36",
    "SCHEDULER": "scrapy_redis.scheduler.Scheduler",
    "DUPEFILTER_CLASS": "scrapy_redis.dupefilter.RFPDupeFilter",
    "SCHEDULER_PERSIST": true,
    "REDIS_START_URLS_AS_ZSET": true,
    "REDIS_START_URLS_KEY": "9dings:start_urls",
    "SCHEDULER_DUPEFILTER_KEY": "currency:9ding:dupefilter",
    "SCHEDULER_QUEUE_KEY": "currency:9ding:requests"
  },
  "start_urls": ["http://www.9dingchem.com/"],
  "allowed_domains": ["9dingchem.com"],
  "srules": "9ding_rules",

  "rules": [
    {
      "link_extractor": {
        "restrict_css": "li[catalogcode=\"12\"] ul"
      },
      "follow": true,
      "callback": "parse_item"
    }
  ],
  "item": {
    "class": "DingItem",
    "loader": "ItemLoader",
    "nextpage": true,
    "next_rule": "ul.pagination li.active + li a::attr('href')",
    "attrs": {
      "source": [
        {
          "method": "value",
          "arg": "9ding"
        }
      ],
      "urls": [
        {
          "method": "css",
          "arg": ".kj_proxjny.c a[id]::attr(\"href\")"
        }
      ]

    }
  }
}